<tbody>
    @if (Model != null && Model.Any())
    {
        foreach (var posgraduacao in Model)
        {
            <tr>
                <th scope="row">@posgraduacao.id</th>
                <td>@posgraduacao.nome</td>
                <td>@posgraduacao.nome_curso</td>
                <td>@posgraduacao.campus_curso</td>
                <td>@posgraduacao.descricao</td>
                <td>@posgraduacao.edital</td>
                <td>@posgraduacao.DataCadastro</td>
                <td>@posgraduacao.DataAtualizacao</td>
                <td>
                    <div>
                        <a role="button" class="btn btn-primary" asp-route-id="@posgraduacao.id" asp-controller="Posgraduacao" asp-action="Editar">Editar</a>
                        <a role="button" class="btn btn-primary" asp-route-id="@posgraduacao.id" asp-controller="Posgraduacao_Professor" asp-action="ListarProfVinculados">Listar</a>
                        <a role="button" class="btn btn-danger" asp-route-id="@posgraduacao.id" onclick="deletar(@posgraduacao.id)">Deletar</a>
                        <a role="button" class="btn btn-primary" asp-route-id="@posgraduacao.id" onclick="incluir(@posgraduacao.id)">Cadastrar-se</a>
                    </div>
                </td>
            </tr>
        }
    }


</tbody>